{
  "name": "blazing-bird",
  "description": "React based SPA for birdeye",
  "version": "1.0.0",
  "author": "ankur.kushwaha@birdeye.com",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "es6",
    "hmr",
    "hot",
    "babel",
    "webpack",
    "sentry",
    "architecture",
    "webpack-dev-middleware",
    "webpack-hot-middleware",
    "boilerplate",
    "starter"
  ],
  "engine-strict": true,
  "engines": {
    "npm": ">=6.0.0",
    "node": ">=6.0.0"
  },
  "scripts": {
    "clean:build": "rimraf build",
    "build:html": "node ./bin/buildHtml.js",
    "prestart": "npm run clean:build",
    "start": "node dev-server.js",
    "webpack": "webpack --config config/webpack.config.development",
    "prebuild": "npm run clean:build",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.production.js --progress",
    "postbuild": "react-snapshot",
    "build:serve": "http-server build -p 3003 --cors -o",
    "lint": "eslint src/**/*.js",
    "test": "mocha --compilers js:babel-core/register,css:./test/unit/cssNullCompiler.js --require ./test/unit/testHelper.js --recursive ./test/unit",
    "test:watch": "npm run test -- --watch",
    "tunnel": "lt --port 3000",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "precommit": "npm run lint"
  },
  "devDependencies": {
    "@storybook/react": "^3.1.3",
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.9",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "compression-webpack-plugin": "^0.4.0",
    "copy-webpack-plugin": "3.0.1",
    "cross-env": "2.0.1",
    "css-loader": "0.24.0",
    "eslint": "3.4.0",
    "eslint-plugin-react": "6.2.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "2.0.0-beta.3",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "0.9.0",
    "husky": "^0.13.4",
    "imports-loader": "^0.7.1",
    "jquery": "3.1.0",
    "jsdom": "9.4.5",
    "mocha": "3.0.2",
    "node-sass": "^4.5.3",
    "postcss-loader": "0.11.1",
    "react-addons-test-utils": "15.3.1",
    "redbox-react": "1.3.0",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-slider-monitor": "1.0.7",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.21",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2",
    "webpack-merge": "0.14.1"
  },
  "dependencies": {
    "autoprefixer": "6.4.1",
    "babel-runtime": "6.11.6",
    "bundle-loader": "^0.5.5",
    "config": "^1.26.1",
    "express": "4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "4.15.0",
    "raven-js": "3.6.1",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "3.0.0-beta.2",
    "react-redux": "4.4.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-snapshot": "^1.1.0",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "redux-promise-middleware": "^4.3.0",
    "reselect": "2.5.3"
  }
}
